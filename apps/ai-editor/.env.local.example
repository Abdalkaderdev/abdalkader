# AI Lab Environment Configuration

# Site Configuration
NEXT_PUBLIC_LAB_SITE_URL=https://lab.abdalkader.dev
NEXT_PUBLIC_SITE_NAME="AI Lab - Abdalkader Alhamoud"
NEXT_PUBLIC_SITE_DESCRIPTION="Interactive AI experiments and machine learning demos"

# Performance Monitoring
NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true
NEXT_PUBLIC_PERFORMANCE_BUDGET_LCP=2000
NEXT_PUBLIC_PERFORMANCE_BUDGET_FID=100
NEXT_PUBLIC_PERFORMANCE_BUDGET_CLS=0.1
NEXT_PUBLIC_PERFORMANCE_BUDGET_BUNDLE_SIZE=150000

# Analytics
NEXT_PUBLIC_GA_TRACKING_ID=G-XXXXXXXXXX
NEXT_PUBLIC_ENABLE_ANALYTICS=true

# TensorFlow.js Configuration
NEXT_PUBLIC_TF_BACKEND=webgl
NEXT_PUBLIC_TF_ENABLE_WASM=true
NEXT_PUBLIC_TF_ENABLE_CPU=true

# Accessibility
NEXT_PUBLIC_ENABLE_ACCESSIBILITY_TESTING=true
NEXT_PUBLIC_ACCESSIBILITY_LEVEL=AA

# Development Tools
NEXT_PUBLIC_ENABLE_DEV_TOOLS=true
NEXT_PUBLIC_SHOW_PERFORMANCE_MONITOR=true
NEXT_PUBLIC_SHOW_BUNDLE_ANALYZER=true
NEXT_PUBLIC_SHOW_ACCESSIBILITY_TESTER=true

# Business Configuration
NEXT_PUBLIC_BUSINESS_EMAIL=hello@abdalkader.dev
NEXT_PUBLIC_BUSINESS_PHONE=+962791234567
NEXT_PUBLIC_BUSINESS_LOCATION="Amman, Jordan"

# API Configuration
NEXT_PUBLIC_API_BASE_URL=/api
NEXT_PUBLIC_ENABLE_MOCK_API=true

# Feature Flags
NEXT_PUBLIC_ENABLE_LEAD_CAPTURE=true
NEXT_PUBLIC_ENABLE_EXPERIMENTS=true
NEXT_PUBLIC_ENABLE_PLAYGROUND=true
NEXT_PUBLIC_ENABLE_BUSINESS_PAGE=true