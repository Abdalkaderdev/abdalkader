{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "pages/api/generate.ts": {
      "maxDuration": 30
    },
    "pages/api/socket.ts": {
      "maxDuration": 10
    }
  }
}