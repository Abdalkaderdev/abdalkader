{
  "buildCommand": "cd ../.. && bash install-pnpm.sh && pnpm turbo run build --filter=@abdalkader/portfolio",
  "outputDirectory": ".next",
  "installCommand": "bash ../../install-pnpm.sh",
  "framework": "nextjs",
  "trailingSlash": true,
  "cleanUrls": true,
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_ENVIRONMENT": "production",
    "NEXT_PUBLIC_DEBUG": "false",
    "NEXT_PUBLIC_FEATURE_FLAGS": "false",
    "NEXT_PUBLIC_PERFORMANCE_MONITORING": "true",
    "NEXT_PUBLIC_ERROR_TRACKING": "true",
    "NEXT_PUBLIC_AB_TESTING": "false"
  }
}
