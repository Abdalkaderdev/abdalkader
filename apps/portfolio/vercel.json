{
  "buildCommand": "cd ../.. && pnpm install --no-frozen-lockfile && pnpm turbo run build --filter=@abdalkader/portfolio",
  "outputDirectory": ".next",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "framework": "nextjs",
  "trailingSlash": true,
  "cleanUrls": true,
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_ENVIRONMENT": "@vercel/git-branch-name",
    "NEXT_PUBLIC_DEBUG": "false",
    "NEXT_PUBLIC_FEATURE_FLAGS": "false",
    "NEXT_PUBLIC_PERFORMANCE_MONITORING": "true",
    "NEXT_PUBLIC_ERROR_TRACKING": "true",
    "NEXT_PUBLIC_AB_TESTING": "false"
  }
}
