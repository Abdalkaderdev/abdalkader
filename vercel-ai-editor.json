{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd apps/ai-editor && npm run build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "outputDirectory": "apps/ai-editor/.next",
  "framework": "nextjs",
  "functions": {
    "apps/ai-editor/pages/api/generate.ts": {
      "maxDuration": 30
    },
    "apps/ai-editor/pages/api/socket.ts": {
      "maxDuration": 10
    }
  }
}